#%RAML 1.0 SecurityScheme
type: OAuth 2.0
description: use token
describedBy:
  headers:
    Authorization:
      description: user must send a valid token
      type: integer
  queryParameters:
    acces_token:
      description: valid token
      type: integer
  responses:
     403:
      description: "credential is requred"
     401:
      description: "token  is  incorrect"
     
